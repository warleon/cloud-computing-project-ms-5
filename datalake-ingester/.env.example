# AWS Configuration
# Si tu EC2 tiene IAM Role, estas credenciales NO son necesarias
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=                    # Dejar vacío si usas IAM Role
AWS_SECRET_ACCESS_KEY=                # Dejar vacío si usas IAM Role
AWS_SESSION_TOKEN=                    # Dejar vacío si usas IAM Role

# S3 Buckets Configuration
# Reemplaza con los nombres de tus buckets S3
S3_BUCKET_MS1=your-bucket-ms1
S3_BUCKET_MS2=your-bucket-ms2
S3_BUCKET_MS3=your-bucket-ms3

# MySQL Database (MS1)
MYSQL_HOST=mysql-container-name
MYSQL_PORT=3306
MYSQL_USER=your_mysql_user
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=your_database_name

# PostgreSQL Database (MS2)
POSTGRES_HOST=postgres-container-name
POSTGRES_PORT=5432
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DATABASE=your_database_name

# MongoDB Database (MS3)
MONGO_HOST=mongo-container-name
MONGO_PORT=27017
MONGO_USER=your_mongo_user
MONGO_PASSWORD=your_mongo_password
MONGO_DATABASE=your_database_name
MONGO_AUTH_SOURCE=admin

# Ingestion Configuration
INGESTION_INTERVAL=3600
ENABLE_PARTITIONING=true
LOG_LEVEL=INFO
