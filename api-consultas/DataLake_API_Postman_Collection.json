		{
			"name": "Ejemplo: JOIN entre tablas",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"query\": \"SELECT u.name, u.email, COUNT(o.id) as total_orders, SUM(o.total_amount) as total_spent FROM mysql_ms1_users u JOIN mysql_ms1_orders o ON u.id = o.user_id GROUP BY u.name, u.email ORDER BY total_spent DESC LIMIT 5\"\n}"
				},
				"url": {
					"raw": "http://50.16.35.196:8000/api/query/custom",
