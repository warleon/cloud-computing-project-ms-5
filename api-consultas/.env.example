# AWS Configuration
# Si tu EC2 tiene IAM Role, estas credenciales NO son necesarias
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=                    # Dejar vacío si usas IAM Role
AWS_SECRET_ACCESS_KEY=                # Dejar vacío si usas IAM Role
AWS_SESSION_TOKEN=                    # Dejar vacío si usas IAM Role

# Athena Configuration
ATHENA_DATABASE=your_database_name
ATHENA_OUTPUT_LOCATION=s3://your-bucket-name/athena-results/
ATHENA_WORKGROUP=primary

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=false

# Logging
LOG_LEVEL=INFO
